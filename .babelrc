{
	"ignore": [
		"node_modules/**/*"
	],
	"presets": [
		["env", {
          "targets": {
            "node": "current",
            "browsers": ["last 2 versions", "safari >= 7"],
            "modules": false
          }
        }],
      "stage-3",
      "react"
    ],
	"plugins": [
		"syntax-dynamic-import",
		"transform-react-jsx"
	]
}
